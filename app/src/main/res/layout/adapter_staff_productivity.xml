<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>
        <variable
            name="staffProductivity"
            type="com.mitkoindo.smartcollection.objectdata.StaffProductivity" />

        <import type="android.view.View"/>
    </data>


    <android.support.v7.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <android.support.constraint.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <android.support.constraint.Guideline
                android:id="@+id/guideline_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.28"/>

            <android.support.constraint.Guideline
                android:id="@+id/guideline_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.36"/>

            <android.support.constraint.Guideline
                android:id="@+id/guideline_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.44"/>

            <android.support.constraint.Guideline
                android:id="@+id/guideline_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.52"/>

            <android.support.constraint.Guideline
                android:id="@+id/guideline_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.60"/>

            <android.support.constraint.Guideline
                android:id="@+id/guideline_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.68"/>

            <android.support.constraint.Guideline
                android:id="@+id/guideline_6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.76"/>

            <android.support.constraint.Guideline
                android:id="@+id/guideline_7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                app:layout_constraintGuide_percent="0.84"/>

            <View
                android:id="@+id/background_nama"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline_name"
                android:background="@color/bg_gray"/>

            <View
                android:id="@+id/background_1"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/guideline_name"
                app:layout_constraintEnd_toStartOf="@+id/guideline_1"
                android:background="@color/light_grey"/>

            <View
                android:id="@+id/background_2"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/guideline_1"
                app:layout_constraintEnd_toStartOf="@+id/guideline_2"
                android:background="@color/bg_gray"/>

            <View
                android:id="@+id/background_3"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/guideline_2"
                app:layout_constraintEnd_toStartOf="@+id/guideline_3"
                android:background="@color/light_grey"/>

            <View
                android:id="@+id/background_4"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/guideline_3"
                app:layout_constraintEnd_toStartOf="@+id/guideline_4"
                android:background="@color/bg_gray"/>

            <View
                android:id="@+id/background_5"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/guideline_4"
                app:layout_constraintEnd_toStartOf="@+id/guideline_5"
                android:background="@color/light_grey"/>

            <View
                android:id="@+id/background_6"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/guideline_5"
                app:layout_constraintEnd_toStartOf="@+id/guideline_6"
                android:background="@color/bg_gray"/>

            <View
                android:id="@+id/background_7"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/guideline_6"
                app:layout_constraintEnd_toStartOf="@+id/guideline_7"
                android:background="@color/light_grey"/>

            <View
                android:id="@+id/background_total"
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@+id/guideline_7"
                app:layout_constraintEnd_toEndOf="parent"
                android:background="@color/bg_gray"/>

            <!--Nama-->
            <TextView
                android:id="@+id/text_view_nama"
                style="@style/FontBold.Medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/padding_medium_large"
                android:paddingBottom="@dimen/padding_medium_large"
                android:paddingLeft="@dimen/padding_medium"
                android:paddingStart="@dimen/padding_medium"
                android:paddingRight="@dimen/padding_medium"
                android:paddingEnd="@dimen/padding_medium"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toStartOf="@+id/guideline_name"
                android:text="@{staffProductivity.fullName}"
                tools:text="Eric Wijaya"
                android:maxLines="1"
                android:background="@color/bg_gray"/>

            <!-- < 10-->
            <TextView
                android:id="@+id/text_view_1"
                style="@style/FontBold.Medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/padding_medium_large"
                android:paddingBottom="@dimen/padding_medium_large"
                android:paddingLeft="@dimen/padding_small"
                android:paddingStart="@dimen/padding_small"
                android:paddingRight="@dimen/padding_small"
                android:paddingEnd="@dimen/padding_small"
                android:gravity="center_horizontal"
                app:layout_constraintStart_toEndOf="@+id/guideline_name"
                app:layout_constraintEnd_toStartOf="@+id/guideline_1"
                android:text="@{staffProductivity.timeRange1}"
                tools:text="7"
                android:maxLines="1"
                android:background="@color/light_grey"/>

            <!-- 10-12-->
            <TextView
                android:id="@+id/text_view_2"
                style="@style/FontBold.Medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/padding_medium_large"
                android:paddingBottom="@dimen/padding_medium_large"
                android:paddingLeft="@dimen/padding_small"
                android:paddingStart="@dimen/padding_small"
                android:paddingRight="@dimen/padding_small"
                android:paddingEnd="@dimen/padding_small"
                android:gravity="center_horizontal"
                app:layout_constraintStart_toEndOf="@+id/guideline_1"
                app:layout_constraintEnd_toStartOf="@+id/guideline_2"
                android:text="@{staffProductivity.timeRange2}"
                tools:text="12"
                android:maxLines="1"
                android:background="@color/bg_gray"/>

            <!-- 12-14-->
            <TextView
                android:id="@+id/text_view_3"
                style="@style/FontBold.Medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/padding_medium_large"
                android:paddingBottom="@dimen/padding_medium_large"
                android:paddingLeft="@dimen/padding_small"
                android:paddingStart="@dimen/padding_small"
                android:paddingRight="@dimen/padding_small"
                android:paddingEnd="@dimen/padding_small"
                android:gravity="center_horizontal"
                app:layout_constraintStart_toEndOf="@+id/guideline_2"
                app:layout_constraintEnd_toStartOf="@+id/guideline_3"
                android:text="@{staffProductivity.timeRange3}"
                tools:text="66"
                android:maxLines="1"
                android:background="@color/light_grey"/>

            <!-- 14-16-->
            <TextView
                android:id="@+id/text_view_4"
                style="@style/FontBold.Medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/padding_medium_large"
                android:paddingBottom="@dimen/padding_medium_large"
                android:paddingLeft="@dimen/padding_small"
                android:paddingStart="@dimen/padding_small"
                android:paddingRight="@dimen/padding_small"
                android:paddingEnd="@dimen/padding_small"
                android:gravity="center_horizontal"
                app:layout_constraintStart_toEndOf="@+id/guideline_3"
                app:layout_constraintEnd_toStartOf="@+id/guideline_4"
                android:text="@{staffProductivity.timeRange4}"
                tools:text="9"
                android:maxLines="1"
                android:background="@color/bg_gray"/>

            <!-- 16-18-->
            <TextView
                android:id="@+id/text_view_5"
                style="@style/FontBold.Medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/padding_medium_large"
                android:paddingBottom="@dimen/padding_medium_large"
                android:paddingLeft="@dimen/padding_small"
                android:paddingStart="@dimen/padding_small"
                android:paddingRight="@dimen/padding_small"
                android:paddingEnd="@dimen/padding_small"
                android:gravity="center_horizontal"
                app:layout_constraintStart_toEndOf="@+id/guideline_4"
                app:layout_constraintEnd_toStartOf="@+id/guideline_5"
                android:text="@{staffProductivity.timeRange5}"
                tools:text="8"
                android:maxLines="1"
                android:background="@color/light_grey"/>

            <!-- 18-20-->
            <TextView
                android:id="@+id/text_view_6"
                style="@style/FontBold.Medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/padding_medium_large"
                android:paddingBottom="@dimen/padding_medium_large"
                android:paddingLeft="@dimen/padding_small"
                android:paddingStart="@dimen/padding_small"
                android:paddingRight="@dimen/padding_small"
                android:paddingEnd="@dimen/padding_small"
                android:gravity="center_horizontal"
                app:layout_constraintStart_toEndOf="@+id/guideline_5"
                app:layout_constraintEnd_toStartOf="@+id/guideline_6"
                android:text="@{staffProductivity.timeRange6}"
                tools:text="54"
                android:maxLines="1"
                android:background="@color/bg_gray"/>

            <!-- > 20-->
            <TextView
                android:id="@+id/text_view_7"
                style="@style/FontBold.Medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/padding_medium_large"
                android:paddingBottom="@dimen/padding_medium_large"
                android:paddingLeft="@dimen/padding_small"
                android:paddingStart="@dimen/padding_small"
                android:paddingRight="@dimen/padding_small"
                android:paddingEnd="@dimen/padding_small"
                android:gravity="center_horizontal"
                app:layout_constraintStart_toEndOf="@+id/guideline_6"
                app:layout_constraintEnd_toStartOf="@+id/guideline_7"
                android:text="@{staffProductivity.timeRange7}"
                tools:text="3"
                android:maxLines="1"
                android:background="@color/light_grey"/>

            <!-- Total-->
            <TextView
                android:id="@+id/text_view_total"
                style="@style/FontBold.Medium"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:paddingTop="@dimen/padding_medium_large"
                android:paddingBottom="@dimen/padding_medium_large"
                android:paddingLeft="@dimen/padding_small"
                android:paddingStart="@dimen/padding_small"
                android:paddingRight="@dimen/padding_small"
                android:paddingEnd="@dimen/padding_small"
                android:gravity="center_horizontal"
                app:layout_constraintStart_toEndOf="@+id/guideline_7"
                app:layout_constraintEnd_toEndOf="parent"
                android:text="@{staffProductivity.total}"
                tools:text="158"
                android:maxLines="1"
                android:background="@color/bg_gray"/>

        </android.support.constraint.ConstraintLayout>

    </android.support.v7.widget.CardView>

</layout>
